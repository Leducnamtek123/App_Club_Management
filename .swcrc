{
    "$schema": "https://json.schemastore.org/swcrc",
    "exclude": "node_modules/",
    "sourceMaps": true,
    "module": {
      "type": "commonjs"
    },
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "target": "es2021",
      "baseUrl": "./src",
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      }
    }
  }